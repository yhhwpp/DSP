{
    "eslint": {
        "globals": {
            "baidu": true,
            "bds": true,
            "T": true,
            "$": true,
            "jQuery": true,
            "goog": true,
            "fis": true,
            "F": true,
            "__inline": true,
            "__uri": true,
            "angular": true,
            "setImmediate": true,
            "ActiveXObject": true,
            "unescape": true,
            "escape": true,
            "API_URL": true,
            "MSG": true,
            "CONSTANT": true,
            "Helper": true,
            "doT": true,
            "dataTable": true,
            "plupload": true,
            "Qiniu": true,
            "kendo": true,
            "BOS": true,
            "OPERATION_LIST": true,
            "TraffickerCommon": true,
            "LANG": true
        },
        "rules": {
            "fecs-indent":  [2, "spaces", 4, 0],
            "fecs-valid-jsdoc": [2, {
                "prefer": {
                    "arg": "param",
                    "argument": "param",
                    "returns": "return",
                    "fileoverview": "file"
                },
                "requireReturn": false,
                "requireReturnDescription": false,
                "requireParamDescription": true
            }],
            "fecs-max-statements": [0, 50, {"ignore": "IIFE"}],
            "fecs-key-spacing": [2, {"beforeColon": false, "afterColon": true, "align": "colon"}],
            "fecs-eol-last": [2, true],
            "fecs-camelcase": [0],
            "fecs-space-infix-ops": 2,
            "fecs-dot-notation": [2, {"allowKeywords": true, "allowPattern": "^catch$"}],
            "fecs-no-reserved-keys": 2,
            "fecs-properties-quote": [
                2,
                {"spread": true, "get": false, "set": false, "method": true, "computed": true}
            ],
            "fecs-no-forin-array": 2,
            "fecs-valid-amd-id": 2,
            "fecs-no-eval": 2,
            "fecs-no-global-require": 2,
            "fecs-jsx-var": 2,
            "fecs-esnext-ext": [2, ["js", "es"]],
            "fecs-no-extra-semi": 2,
            "fecs-no-arguments": 2,
            "fecs-use-method-definition": 2,
            "fecs-use-property-shorthand": [
                1,
                {"spread": true, "get": false, "set": false, "method": false, "computed": false}
            ],
            "fecs-prefer-class": 2,
            "fecs-prefer-super": 2,

            "no-console": 1,
            "no-constant-condition": 1,
            "comma-dangle": 2,
            "no-debugger": 2,
            "no-dupe-keys": 1,
            "no-empty-character-class": 2,
            "no-ex-assign": 2,
            "no-extra-boolean-cast": 1,
            "no-func-assign": 1,
            "no-inner-declarations": 1,
            "no-invalid-regexp": 2,
            "no-negated-in-lhs": 2,
            "no-obj-calls": 2,
            "no-sparse-arrays": 2,
            "no-unreachable": 2,
            "use-isnan": 2,

            "valid-typeof": 2,

            "curly": [2, "all"],
            "eqeqeq": [2, "allow-null"],
            "guard-for-in": 0,
            "no-else-return": 1,
            "no-empty-label": 1,
            "no-eval": 1,
            "no-extend-native": 0,
            "no-extra-bind": 1,
            "no-implied-eval": 1,
            "no-iterator": 2,
            "no-irregular-whitespace": 1,
            "no-lone-blocks": 1,
            "no-loop-func": 1,
            "no-multi-str": 1,
            "no-native-reassign": 2,
            "no-new-wrappers": 2,
            "no-octal": 1,
            "no-octal-escape": 1,
            "no-proto": 2,
            "no-redeclare": 1,
            "no-self-compare": 2,
            "no-unneeded-ternary": 2,
            "no-with": 1,
            "radix": 2,
            "wrap-iife": [2, "any"],

            "no-delete-var": 1,
            "no-dupe-args": 2,
            "no-duplicate-case": 2,
            "no-label-var": 1,
            "no-shadow-restricted-names": 2,
            "no-undef": 2,
            "no-undef-init": 1,
            "no-unused-vars": [1, {"vars": "local", "args": "none"}],
            "no-use-before-define": [2, "nofunc"],

            "brace-style": [1, "stroustrup", {}],
            "comma-spacing": [2, {"before": false, "after": true}],
            "comma-style": [2, "last"],
            "new-cap": [2, {"capIsNewExceptions": ["T", "AddToFavoritesBar"]}],
            "new-parens": 1,
            "no-array-constructor": 2,
            "no-new-object": 2,
            "no-spaced-func": 2,
            "no-trailing-spaces": 2,
            "no-extra-parens": [2, "functions"],
            "no-mixed-spaces-and-tabs": 2,
            "one-var": [2, "never"],
            "operator-linebreak": [2, "before"],
            "quotes": [2, "single"],
            "semi": [2, "always"],
            "semi-spacing": 2,
            "space-after-keywords": [2, "always"],
            "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
            "space-before-blocks": [2, "always"],
            "array-bracket-spacing": [2, "never"],
            "computed-property-spacing": [2, "never"],
            "object-curly-spacing": [2, "never"],
            "space-in-parens": [2, "never"],
            "space-return-throw-case": 1,
            "space-unary-ops": 1,
            "spaced-comment": [2, "always", {"exceptions": ["-", "+", ""]}],
            "max-nested-callbacks": [1, 3],

            "max-depth": [1, 10],
            "max-len": [0, 120, 4, {"ignoreUrls": true, "ignoreComments": true}],
            "max-params": [1, 8],

            "arrow-parens": [2, "as-needed"],
            "arrow-spacing": 2,
            "constructor-super": 2,
            "generator-star-spacing": [2, {"before": false, "after": true}],
            "no-arrow-condition": 2,
            "no-class-assign": 1,
            "no-const-assign": 2,
            "no-dupe-class-members": 1,
            "no-this-before-super": 1,
            "no-var": 1
        }
    },
     "csshint": {
        "require-number": ["font-weight"],
        "horizontal-vertical-position": false,
        "hex-color": false,
        "disallow-important": false,
        "disallow-overqualified-elements": false,
        "require-newline": [],
        "max-length": 500,
        "max-selector-nesting-level": 4
    },
    "htmlcs": {
        "css-in-head": false,
        "script-in-tail": false
    }
}